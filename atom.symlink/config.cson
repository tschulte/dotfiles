"*":
  "atom-typing-speedometer":
    showSecondAvg: true
  "autocomplete-plus":
    enableAutoActivation: false
    enableAutoConfirmSingleSuggestion: false
  core:
    disabledPackages: [
      "background-tips"
      "exception-reporting"
      "metrics"
    ]
    followSymlinks: false
    hideGitIgnoredFiles: true
    telemetryConsent: "no"
  editor:
    fontFamily: "Inconsolata"
    fontSize: 15
    invisibles: {}
    preferredLineLength: 120
    showIndentGuide: true
    showInvisibles: true
    softWrap: true
    softWrapHangingIndent: 8
    tabLength: 4
  "linter-ui-default":
    panelHeight: 69
  welcome:
    showOnStartup: false
