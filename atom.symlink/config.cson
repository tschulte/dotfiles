"*":
  core:
    disabledPackages: [
      "background-tips"
      "exception-reporting"
      "metrics"
    ]
    followSymlinks: false
    hideGitIgnoredFiles: true
    telemetryConsent: "no"
  editor:
    fontFamily: "Inconsolata"
    fontSize: 15
    invisibles: {}
    preferredLineLength: 120
    showIndentGuide: true
    showInvisibles: true
    softWrap: true
    softWrapHangingIndent: 8
    tabLength: 4
  welcome:
    showOnStartup: false
