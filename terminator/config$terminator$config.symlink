[global_config]
  enabled_plugins = LaunchpadCodeURLHandler, APTURLHandler, MavenPluginURLHandler, LaunchpadBugURLHandler
[keybindings]
  broadcast_all = None
  broadcast_group = None
  broadcast_off = None
  edit_tab_title = None
  edit_terminal_title = None
  edit_window_title = None
  group_all = None
  group_tab = None
  hide_window = None
  insert_number = None
  insert_padded = None
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      profile = default
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
[profiles]
  [[default]]
    background_darkness = 0.8
    cursor_color = "#aaaaaa"
    font = Monospace 16
    foreground_color = "#ffffff"
    use_system_font = False
  [[presentation]]
    background_color = "#ffffff"
    cursor_color = "#aaaaaa"
    font = Monospace 20
    foreground_color = "#000000"
    use_system_font = False
